Options Includes FollowSymLinks MultiViews
#Force www:
RewriteEngine on

# If https send to https://www.
RewriteCond %{HTTP_HOST} ^miraclesoft.com$ [NC]
RewriteCond %{HTTPS} on
RewriteRule ^(.*)$ https://www.miraclesoft.com/$1 [R=301,L]
# If http send to http://www.
RewriteCond %{HTTP_HOST} ^miraclesoft.com$ [NC]
RewriteCond %{HTTPS} off
RewriteRule ^(.*)$ http://www.miraclesoft.com/$1 [R=301,L]
#RewriteCond %{HTTP_HOST} ^miraclesoft.com [NC]
